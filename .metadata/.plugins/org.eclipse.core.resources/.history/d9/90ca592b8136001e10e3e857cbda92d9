package exer2;

import java.util.Arrays;

public class Student {
	String name;
	int [] jum;
//		int tot;
//		double avg;
	
	// 입력이 필요한 필드와 그에 따른 연산
	public Student(String name, String ... jum1) {
		this.name = name;
		this.jum = new int[jum1.length];
    	for(int i : jum) {
    		jum[i] = Integer.parseInt(jum1[i]);
	    }
	}

	@Override
	public String toString() {
		return "Student [name=" + name + ", jum=" + Arrays.toString(jum) + "]";
	}
	
}