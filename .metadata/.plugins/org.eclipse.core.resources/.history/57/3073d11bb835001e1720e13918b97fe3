package exercise;

public class Bike3 implements Bike0{
	String name;
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = "세발자전거";
	}
	
	@Override
	public String toString() {
		return name + "(" + tire + ", " + handle + ", " + basket + ")" + intf;
	}
	@Override
	public void go() {
		this.intf = "가 간다";
		
	}
	@Override
	public void fix() {
		this.intf = "를 고친다";
		
	}
	@Override
	public void fall() {
		this.intf = "가 넘어진다";
		
	}
}
