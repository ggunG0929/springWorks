package exercise;

public class Bike1 implements Bike0{
	String intf = "";

	@Override
	public String toString() {
		return "MTB 자전거" + intf;
	}
	@Override
	public void go() {
		this.intf = "가 산 길 속을 헤쳐간다";
		toString();
		
	}
	@Override
	public void fix() {
		this.intf = "를 뚱땅뚱땅 고친다";
		toString();
		
	}
	@Override
	public void fall() {
		this.intf = "가 우르르 넘어진다";
		toString();
	}
	
	private void sysout() {
		// TODO Auto-generated method stub

	}
}
