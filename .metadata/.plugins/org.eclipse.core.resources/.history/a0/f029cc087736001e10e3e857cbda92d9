package exer2;

public class SRank {
	String name;
	Kor kor;
	Eng eng;
	Mat mat;
	int sum, rank;
	double avg;
	
	public SRank(String name, Kor kor, Eng eng, Mat mat) {
		super();
		this.name = name;
		calc();
	}
	void calc(){
		sum = 0;
		sum = kor.getJum()+eng.getJum()+mat.getJum();
		avg = sum/3;
	}
	void clacRank() {
		
	}
	
	
	class Kor{
		int jum;
		public int getJum() {
			return jum;
		}
		public void setJum(int jum) {
			this.jum = jum;
		}
		@Override
		public String toString() {
			return "Kor [jum=" + jum + "]";
		}

	}
	class Eng{
		int jum;

		public int getJum() {
			return jum;
		}

		public void setJum(int jum) {
			this.jum = jum;
		}

		@Override
		public String toString() {
			return "Eng [jum=" + jum + "]";
		}
		
	}
	class Mat{
		int jum;

		public int getJum() {
			return jum;
		}

		public void setJum(int jum) {
			this.jum = jum;
		}

		@Override
		public String toString() {
			return "Mat [jum=" + jum + "]";
		}
		
	}
	@Override
	public String toString() {
		return "Student [name=" + name + ", kor=" + kor + ", eng=" + eng + ", mat=" + mat + ", sum=" + sum + ", rank="
				+ rank + ", avg=" + avg + "]";
//				+ rank + ", avg=" + avg + ", rank=" + rank + "]";
	}

}
