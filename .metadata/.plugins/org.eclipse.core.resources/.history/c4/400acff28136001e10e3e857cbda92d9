package exer2;

import java.util.Arrays;

public class Student {
	String name;
	int [] jum;

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int[] getJum() {
		return jum;
	}

	public void setJum(int[] jum) {
		this.jum = jum;
	}

	@Override
	public String toString() {
		return "Student [name=" + name + ", jum=" + Arrays.toString(jum) + "]";
	}
	
}