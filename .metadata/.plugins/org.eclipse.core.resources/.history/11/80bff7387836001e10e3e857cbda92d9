package exer2;

import java.util.Arrays;

public class SRank {
	Student st;
	int tot, rank;
	double avg;
	
	class Student{
		String name;
		int [] jum;
		calc();
		
	}
	void calc() {
		for(int i : jum) {
			tot += i;
		}
		avg = (double)tot/jum.length;
	}
		
		public String getName() {
			return name;
		}
		public void setName(String name) {
			this.name = name;
		}
		public int[] getJum() {
			return jum;
		}
		public void setJum(int... jum) {
			this.jum = jum;
		}
		
		public int getTot() {
			return tot;
		}
		public void setTot(int tot) {
			this.tot = tot;
		}
		public double getAvg() {
			return avg;
		}
		public void setAvg(double avg) {
			this.avg = avg;
		}
		public int getRank() {
			return rank;
		}
		public void setRank(int rank) {
			this.rank = rank;
		}
		@Override
		public String toString() {
			return "Student [name=" + name + ", jum=" + Arrays.toString(jum) + "]";
		}
		
	}


}
